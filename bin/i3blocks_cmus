#!/bin/bash
#status="$(cmus-remote -C status | grep status | cut -d ' ' -f2)"
file="$(cmus-remote -C status | grep file | grep -o '[^/]*$')"

foreground=$(xrdb -query | grep 'color0:'| awk '{print $NF}')
background=$(xrdb -query | grep 'color4:'| awk '{print $NF}')

if [ -z "$file" ]
then
	echo " no music "
	echo " no music "
else
	echo " $file "
	echo " $file "
fi

echo "$foreground"
echo "$background"


